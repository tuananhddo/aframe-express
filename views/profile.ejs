<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <script src="https://aframe.io/releases/1.0.4/aframe.min.js"></script>
    <script>
        const BASE_URL = window.location.protocol + "//" + window.location.host;
    </script>
    <script src="/js/control.js"></script>
    <script src="/js/aframe-dialog-popup-component.js"></script>
    <script src="/js/getCreateEntity.js"></script>

</head>
<body>
<!--<a-scene inspector="url: http://localhost:3333/dist/aframe-inspector.js">-->

<a-scene id='mainUserScene' background="color: #FAFAFA">
    <a-entity id="rig" wasd-controls camera position="0 2 0" look-controls>
        <a-entity id="mouseCursor" cursor="rayOrigin: mouse"></a-entity>
    </a-entity>
    <a-assets>
        <a-asset-item id="door-glb" src="/models/baseCreator/door.glb"></a-asset-item>
        <a-asset-item id="base-glb" src="/models/baseCreator/baseForObJect.glb"></a-asset-item>
        <audio id="river" src="https://3d-model-cdn.s3-ap-southeast-1.amazonaws.com/bensound-ukulele.mp3" preload="auto"></audio>
        <video id="penguin-sledding" autoplay loop="true" src="/video/a.mp4"></video>

    </a-assets>
    <a-box id="box"
           position="0 2 0"
           height="2"
           color="#FFC65D"
           sound="src: #river"
           audiohandler=""
    >
    </a-box>
    <a-video src="#penguin-sledding" audio-stopper width="16" height="9" position="0 0 -20"></a-video>

</a-scene>
<script>
    // const BASE_URL = window.location.protocol + "//" + window.location.host;
    const scene = document.querySelector('#mainUserScene');
    const inspectorURL = `url: ${BASE_URL}/js/aframe-inspector-local.min.js`;
    scene.setAttribute('inspector', inspectorURL);
    console.log(scene)
    var inspector = scene.components.inspector;

    scene.addEventListener('loaded', function () {
        scene.setAttribute('inspector', inspectorURL);

    });</script>
<!--<script src=" https:
    //unpkg.com/aframe-room-component/dist/aframe-room-component.min.js"></script>-- >

< /body>
< /html>

